# Google Gemini API Configuration
VITE_GEMINI_API_KEY=your_gemini_api_key_here
# Optional: Specify model name for Nano Banana Pro (if you have a subscription)
# Examples: gemini-3.0-pro-image, gemini-3-pro-image, gemini-2.5-flash-image-pro
# Leave empty to use default: gemini-2.5-flash-image-preview
# VITE_GEMINI_MODEL=gemini-3.0-pro-image

# Google Cloud Configuration (for Imagen background removal)
VITE_GOOGLE_CLOUD_PROJECT_ID=your_google_cloud_project_id_here
VITE_GOOGLE_CLOUD_LOCATION=us-central1

# Firebase Configuration
VITE_FIREBASE_API_KEY=your_firebase_api_key_here
VITE_FIREBASE_AUTH_DOMAIN=your_project_id.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your_firebase_project_id_here
VITE_FIREBASE_STORAGE_BUCKET=your_project_id.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=your_messaging_sender_id_here
VITE_FIREBASE_APP_ID=your_firebase_app_id_here

# Stripe Configuration
VITE_STRIPE_PUBLISHABLE_KEY=pk_test_your_stripe_publishable_key_here

# Stripe Secret Keys (for server-side functions)
STRIPE_SECRET_KEY=sk_test_your_stripe_secret_key_here
STRIPE_WEBHOOK_SECRET=whsec_your_webhook_secret_here

# Firebase Configuration (for webhook functions)
FIREBASE_API_KEY=your_firebase_api_key_here
FIREBASE_AUTH_DOMAIN=your_project_id.firebaseapp.com
FIREBASE_PROJECT_ID=your_project_id_here
FIREBASE_STORAGE_BUCKET=your_project_id.appspot.com
FIREBASE_MESSAGING_SENDER_ID=your_sender_id_here
FIREBASE_APP_ID=your_app_id_here

# Application Configuration
VITE_APP_NAME=Character Studio
VITE_APP_VERSION=1.0.0
